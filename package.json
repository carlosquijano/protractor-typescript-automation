{
  "name": "protractor-typescript-automation",
  "version": "0.1.0",
  "description": "Template for TypeScript projects that uses Protractor",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p ./",
    "lint": "tslint --project tsconfig.json --config tslint.json 'e2e/**/*.ts' --fix",
    "e2e" : "npm run build && npm run lint && protractor ./build/protractor.conf.js",
    "test": "npm run e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlosquijano/protractor-typescript-automation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlosquijano/protractor-typescript-automation/issues"
  },
  "homepage": "https://github.com/carlosquijano/protractor-typescript-automation#readme",
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "@types/node": "^11.9.5",
    "protractor": "^5.4.2",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  }
}
